{
  "name": "gatsby-blog",
  "description": "my blog",
  "version": "1.0.0",
  "private": true,
  "author": "Kenshiro Sugiura <ken41shiro@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "b:s": "yarn build && yarn serve",
    "clean": "rimraf public && rimraf .cache",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "yarn clean && gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "yarn dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.4",
    "babel-plugin-styled-components": "^1.11.1",
    "classnames": "^2.2.6",
    "contentful": "^7.14.8",
    "dotenv": "^8.2.0",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-canonical-urls": "^2.3.13",
    "gatsby-plugin-emotion": "^4.3.15",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-typescript": "^2.5.0",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-images": "^3.4.1",
    "gatsby-remark-prismjs": "^3.6.0",
    "gatsby-remark-responsive-iframe": "^2.4.17",
    "gatsby-remark-smartypants": "^2.3.13",
    "gatsby-source-contentful": "^3.1.2",
    "gatsby-source-filesystem": "^2.4.1",
    "gatsby-transformer-json": "^2.4.15",
    "gatsby-transformer-remark": "^2.9.2",
    "gatsby-transformer-sharp": "^2.5.21",
    "modern-normalize": "^1.0.0",
    "normalize.css": "^8.0.1",
    "polished": "^4.0.3",
    "prism-themes": "^1.5.0",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-gatsby": "^0.5.16",
    "eslint": "^7.12.1",
    "eslint-config-blvd": "^1.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  }
}
